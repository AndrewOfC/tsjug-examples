  "bugs": {
    "url": "https://github.com/AndrewOfC/tsjug-examples/issues"
  },
  "homepage": "https://github.com/AndrewOfC/tsjug-examples#readme",
  "dependencies": {
    "@tsjug/runtime": "^1.0.26",
    "@types/node": "^22.10.2"
  },
  "devDependencies": {
    "@tsjug/cli": "^1.0.3",
    "tsx": "^4.19.2"
  }
}
